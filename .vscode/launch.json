	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Event Bus",
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"start-dev"
			],
			"skipFiles": [
				"<node_internals>/**"
			],
			"port": "9229",
			"protocol": "inspector",
			"console": "integratedTerminal"
		}
	]
}